PORT=3000
NODE_ENV=production

# Master key for registering new users
MASTER_KEY=your-secure-master-key-here

# JWT Secret (will be auto-generated if not provided)
JWT_SECRET=your-jwt-secret-here

# Encryption secret for additional security
ENCRYPTION_SECRET=your-encryption-secret-here

# API Keys to protect
OPENAI_API_KEY=sk-your-openai-api-key
ANTHROPIC_API_KEY=sk-ant-your-anthropic-api-key

# Allowed origins (comma-separated)
ALLOWED_ORIGINS=https://yourdomain.com,https://app.yourdomain.com

# Valid API Keys (JSON object, managed by the app)
VALID_API_KEYS={}